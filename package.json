{
  "name": "easy-service-solution",
  "version": "0.0.1",
  "author": "Ionic Framework",
  "homepage": "https://ionicframework.com/",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 8080",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "e2e": "concurrently \"npm run start\" \"wait-on http-get://localhost:8100 && npm run cy:run\" --kill-others --success first",
    "generate:schema": "npx ng-openapi-gen --input ../backend/schema.yaml --output ./src/api",
    "i18n:init": "ngx-translate-extract --input ./src --output ./src/assets/i18n/en.json --key-as-default-value --sort --format json",
    "i18n:extract": "ngx-translate-extract --input ./src --output ./src/assets/i18n/de.json --key-as-default-value --sort --format json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.0",
    "@angular/cdk": "^11.2.8",
    "@angular/common": "~11.2.0",
    "@angular/core": "~11.2.0",
    "@angular/forms": "~11.2.0",
    "@angular/material": "^11.2.8",
    "@angular/platform-browser": "~11.2.0",
    "@angular/platform-browser-dynamic": "~11.2.0",
    "@angular/router": "~11.2.0",
    "@auth0/angular-jwt": "^5.0.2",
    "@capacitor-community/fcm": "^2.0.0",
    "@capacitor/android": "3.0.0-rc.0",
    "@capacitor/browser": "^0.4.10",
    "@capacitor/camera": "^0.4.4",
    "@capacitor/core": "3.0.0-rc.0",
