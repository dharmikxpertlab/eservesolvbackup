    "@capacitor/push-notifications": "^0.3.10",
    "@capacitor/splash-screen": "^0.3.6",
    "@capacitor/storage": "^0.3.6",
    "@eve-sama/ngx-signature-pad": "^2.0.1",
    "@ionic/angular": "^5.5.2",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "d3": "^6.7.0",
    "material-icons": "^0.6.1",
    "moment": "^2.29.1",
    "ng-openapi-gen": "^0.17.0",
    "ngx-otp-input": "^0.9.1",
    "ngx-permissions": "^8.1.1",
    "ngx-pipes": "^2.7.5",
    "rxjs": "~6.6.0",
